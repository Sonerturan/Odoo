<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Sequences for hospital.patient -->
    <record id="seq_hospital_patient" model="ir.sequence">
        <field name="name">Hospital Patient Order</field>
        <field name="code">hospital.patient</field>
        <field name="prefix">HP</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_hospital_appointment" model="ir.sequence">
        <field name="name">Hospital Appointment Order</field>
        <field name="code">hospital.appointment</field>
        <field name="prefix">OP</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>